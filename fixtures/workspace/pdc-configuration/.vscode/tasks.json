{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "pdc",
      "problemMatcher": ["$pdc-lua", "$pdc-external"],
      "label": "Playdate: Build",
      "strip": true,
      "noCompress": true,
      "verbose": true,
      "quiet": true,
      "skipUnknown": true,
      "incrementBuildNumber": true,
      "libPath": ["/path/to/my/libs"]
    }
  ]
}
